{
  "version": "1.0.6-*",
  "authors": [ "Lewis Zou", "Walterwhatwater", "xiaohuan0204" ],
  "description": "Offical package. DotnetSpider is a high performance, light weight cralwer developed by C#.",
  "packOptions": {
    "licenseUrl": "https://raw.githubusercontent.com/zlzforever/DotnetSpider/master/LICENSE",
    "repository": { "url": "https://github.com/zlzforever/DotnetSpider" },
    "owners": [ "zlzforever@163.com" ],
    "tags": [ "DotnetSpider", "Crawler", "Cross Platform", ".Net Core" ]
  },
  "buildOptions": {
    "emitEntryPoint": false,
    "debugType": "portable"
  },
  "dependencies": {
    "DotnetSpider2.Core": "*",
    "DotnetSpider2.Redial": "*",
    "MongoDB.Driver": "2.4.2",
    "Newtonsoft.Json": "9.0.1"
  },
  "frameworks": {
    "net45": {
      "dependencies": {
        "HtmlAgilityPack.CssSelectors": "1.0.2",
        "MySql.Data": "6.9.9",
        "Selenium.WebDriver": "3.0.1",
        "FiddlerCore2": "1.0.0"
      },
      "frameworkAssemblies": {
        "System.Data": "4.0.0.0"
      },
      "buildOptions": {
        "define": [
          "NET_45"
        ]
      }
    },
    "net451": {
      "dependencies": {
        "HtmlAgilityPack.CssSelectors": "1.0.2",
        "MySql.Data": "6.9.9",
        "Selenium.WebDriver": "3.0.1",
        "FiddlerCore2": "1.0.0"
      },
      "frameworkAssemblies": {
        "System.Data": "4.0.0.0"
      },
      "buildOptions": {
        "define": [
          "NET_45"
        ]
      }
    },
    "netstandard1.6": {
      "imports": "portable-net45+win8+dnxcore50",
      "dependencies": {
        "MailKit": "1.10.2",
        "Microsoft.CSharp": "4.3.0",
        "Microsoft.Extensions.DependencyInjection": "1.1.0",
        "Microsoft.Extensions.DependencyInjection.Abstractions": "1.1.0",
        "MySql.Data": "7.0.6-IR31",
        "StackExchange.Redis": "1.2.0",
        "System.Data.Common": "4.3.0",
        "System.Data.SqlClient": "4.3.0",
        "System.Dynamic.Runtime": "4.3.0",
        "System.Runtime.Extensions": "4.3.0",
        "System.Runtime.Serialization.Primitives": "4.3.0",
        "System.Xml.XDocument": "4.3.0"
      },
      "buildOptions": {
        "define": [
          "NET_CORE"
        ]
      }
    }
  }
}